Cell["\<\
How mixing is matrix M? (1 = perfect mixing)\
\>", "Section",
 CellChangeTimes->{{3.605396522876443*^9, 3.6053965576975813`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"mixing", "[", "M_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    SuperscriptBox["4", "3"]], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "4"], 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "1"}], "4"], 
       RowBox[{"Abs", "[", 
        RowBox[{"M", "\[LeftDoubleBracket]", 
         RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "]"}]}]}], ")"}],
     "2"]}]}]], "Input"]
}, Open  ]],

